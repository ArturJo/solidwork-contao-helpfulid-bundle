{
  "name": "solidwork/contao-helpfulid-bundle",
  "description": "This contao bundle adds helpful IDs to backend (pages, articles and modules)",
  "type": "contao-bundle",
  "license": "LGPL-3.0-or-later",
  "autoload": {
    "psr-4": {
      "SolidWork\\ContaoHelpfulidBundle\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Artur Jozefiak"
    }
  ],
  "require": {
    "php": ">=8.1",
    "contao/core-bundle": "^5.3"
  },
  "repositories": {
    "solidwork/contao-helpfulid-bundle": {
      "type": "path",
      "url": "https://github.com/ArturJo/solidwork-contao-helpfulid-bundle.git"
    }
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "php-http/discovery": true
    }
  },
  "extra": {
    "contao-manager-plugin": "SolidWork\\ContaoHelpfulidBundle\\ContaoManager\\Plugin"
  }
}
